<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/locationSettingFragment">

    <fragment
        android:id="@+id/DashBoardFragment"
        android:name="com.curic4t.android.workout.view.main.ui.main.dashboard.DashBoardFragment"
        android:label="main_fragment"
        tools:layout="@layout/fragment_main" />

    <fragment
        android:id="@+id/locationSettingFragment"
        android:name="com.curic4t.android.workout.view.main.ui.main.location.LocationSettingFragment"
        android:label="location_setting_fragment"
        tools:layout="@layout/fragment_location_setting">
        <action
            android:id="@+id/action_locationSettingFragment_to_mainFragment"
            app:destination="@id/DashBoardFragment"
            app:enterAnim="@android:anim/fade_in"
            app:exitAnim="@android:anim/fade_out" />

    </fragment>
    <argument
        android:name="LocationSettingToDashBoard"
        app:argType="com.curic4t.android.workout.view.main.ui.main.location.LocationSettingToDashBoardArguments" />

</navigation>